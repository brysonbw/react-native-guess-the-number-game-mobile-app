{
  "expo": {
    "name": "Guess The Number Game",
    "slug": "guess-the-number-game",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/guess-the-number-logo.png",
    "splash": {
      "image": "./assets/guess-the-number-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
